{
  "name": "secure-communication-kit",
  "version": "1.0.1",
  "description": "Tools for secure communication between peers",
  "types": "out/index.d.ts",
  "source": "src/index.ts",
  "main": "out/index.js",
  "module": "out/index.js",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./out/index.js",
      "import": "./out/esm/index.js"
    },
    "./out/index.js": {
      "require": "./out/index.js",
      "import": "./out/esm/index.js"
    }
  },
  "targets": {
    "binary": {
      "source": "bin/index.ts"
    },
    "main": {},
    "module": {}
  },
  "keywords": [
    "securepeerjs",
    "peerjs",
    "libsodium",
    "hybrid",
    "security",
    "encrypt",
    "sign",
    "handshake"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bosskabouter/SecurePeerJS/tree/master/packages/securepeerkey"
  },
  "scripts": {
    "build": "tsc & tsc --module es2015 --outDir out/esm",
    "coverage":"jest --coverage",
    "dev": "tsc --watch & tsc --watch  --module es2015 --outDir out/esm",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "author": "bosskabouter@protonmail.com",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/jest": "^29.5.0",
    "@types/libsodium-wrappers": "^0.7.10",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "libsodium-wrappers": "^0.7.11"
  },
  "$schema": "https://json.schemastore.org/package"
}
